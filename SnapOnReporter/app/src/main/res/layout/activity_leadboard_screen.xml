<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/size_bg"
    >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="6">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="fill_parent">
            <ImageView
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="fill_parent"
                android:src="@drawable/babyninja"
                android:id="@+id/ladder_image"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:id="@+id/email_ladder"
                android:textSize="18dp"
                android:textColor="@color/white"/>
            <TextView
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_height="0dp"

                android:textSize="12dp"
                android:textColor="@color/white"
                android:id="@+id/level_ladder"
                />
            <TextView
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_height="0dp"
                android:id="@+id/rank_ladder"
                android:textSize="12dp"
                android:textColor="@color/white"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"></LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        >
        <TextView
            android:id="@+id/ninjatext"
            android:layout_width="0dp"
            android:layout_weight="8"
            android:layout_height="wrap_content"
            android:text="Ninja Reporters"
            android:textSize="20dp"
            android:textColor="@color/white"/>
        <TextView android:textColor="@color/white"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="(This Week)"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"></LinearLayout>
